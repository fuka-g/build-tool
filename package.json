{
  "name": "build-tool",
  "version": "1.0.2",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "start": "node .",
    "test": "jest",
    "build": "tsc"
  },
  "buildConfig": {
    "src": "test/src/",
    "dest": "test/dest/",
    "obfuscate": "build",
    "parameters": [
      "dev"
    ],
    "obfuscationParameters": {
      "compact": false,
      "controlFlowFlattening": true,
      "controlFlowFlatteningThreshold": 1,
      "numbersToExpressions": true,
      "simplify": true,
      "stringArrayShuffle": true,
      "splitStrings": true,
      "stringArrayThreshold": 1
    }
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "fs-extra": "^10.0.1",
    "readdirp": "^3.6.0"
  },
  "devDependencies": {
    "javascript-obfuscator": "^4.0.0",
    "jest": "^27.5.1"
  }
}
